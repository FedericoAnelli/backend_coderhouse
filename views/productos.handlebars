<h1 class="m-5">Productos</h1>

<div class="m-5">
    <table class="table table-dark table-hover table-striped align-middle  text-center">
        <thead>
            <tr>
                <th scope="col">Nombre</th>
                <th scope="col">Precio</th>
                <th scope="col">Imagen</th>
            </tr>
        </thead>
        <tbody>
            {{#each productos}}
                <tr>
                    <td>{{ title }}</td>
                    <td>${{ price }}</td>
                    <td><img class="h-25 w-25" src="{{ thumbnail }}" alt={{ title }}></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>